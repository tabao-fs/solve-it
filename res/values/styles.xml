<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="a_blue">
        <item name="android:background">@drawable/a_blue</item>
    </style>
    
    <style name="a_green">
        <item name="android:background">@drawable/a_green</item>
    </style>
    
    <style name="a_orange">
        <item name="android:background">@drawable/a_orange</item>
    </style>
    
    <style name="a_purple">
        <item name="android:background">@drawable/a_purple</item>
    </style>
    
    <style name="a_red">
        <item name="android:background">@drawable/a_red</item>
    </style>
    
    <style name="b_blue">
        <item name="android:background">@drawable/b_blue</item>
    </style>
    
    <style name="b_green">
        <item name="android:background">@drawable/b_green</item>
    </style>
    
    <style name="b_red">
        <item name="android:background">@drawable/b_red</item>
    </style>
    
    <style name="b_yellow">
        <item name="android:background">@drawable/b_yellow</item>
    </style>
    
    <style name="c_flesh">
        <item name="android:background">@drawable/c_flesh</item>
    </style>
    
</resources>
